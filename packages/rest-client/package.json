{
  "name": "@pixelrpg/rest-client",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "typegen:openapi-generator": "openapi-generator-cli generate -i ../../apps/strapi/src/extensions/documentation/documentation/1.0.0/full_documentation.json -g typescript-fetch -o ./dist/clients --skip-validate-spec",
    "typegen:openapi-typescript": "openapi-typescript ../../apps/strapi/src/extensions/documentation/documentation/1.0.0/full_documentation.json --output ./dist/openapi-typescript.ts",
    "typegen:oapi3ts": "node oapi3ts.js --srcPath ../../apps/strapi/src/extensions/documentation/documentation/1.0.0/full_documentation.json --destPath ./dist/oapi3ts --separatedFiles true",
    "typegen:dtsgenerator": "dtsgen --out ./src/types/strapi-rest.ts ../../apps/strapi/src/extensions/documentation/documentation/1.0.0/full_documentation.json",
    "typegen": "yarn run typegen:dtsgenerator",
    "build": "yarn run typegen"
  },
  "devDependencies": {
    "@codegena/oapi3ts-cli": "^2.1.10-alpha.8",
    "@openapitools/openapi-generator-cli": "^2.4.18",
    "dtsgenerator": "^3.13.2",
    "openapi-typescript": "^4.4.0"
  }
}
